         CC = gcc
  CCOPTIONS = -ansi
STD_DEFINES = -Dlinux -D__amd64__ -D_POSIX_C_SOURCE=199309L 				-D_POSIX_SOURCE 				-D_DEFAULT_SOURCE $(PROJECT_DEFINES)
