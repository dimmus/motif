! HISTORY
module latin_strings
    version = 'v1.0'
    names = case_sensitive

value
    local_font : exported
        font('*helvetica-medium-r-normal--14-*-iso8859-1');

    string_direction : exported XmSTRING_DIRECTION_L_TO_R ;

    hello_label : exported compound_string('Hej',separate=true)
						  & 'världen!';

    bye_label : exported compound_string('Adjö',separate=true) &
    'grymma värld!';

    label_label :  exported compound_string('Tryck på knappen en gång',
                              separate=true) &
		            compound_string('för att ändra texten;',separate=true) &
		                  'två gånger för att avsluta';

end module ;
