if ( $MOTIF_HOME != "" ) then
setenv AUTOVPATH $MOTIF_HOME/tests/visuals/Toolkit/Notebook
else
setenv AUTOVPATH /proj/motif/tests/visuals/Toolkit/Notebook
endif
setenv TOP ../../..

set Notebook1a = 'Notebook1 -u 1 -I Notebook1a.Dat -S Notebook1a.Scr -O Notebook1a.prt -V Notebook1a.vis'

set Notebook1b = 'Notebook1 -u 2 -I Notebook1b.Dat -S Notebook1b.Scr -O Notebook1b.prt -V Notebook1b.vis'

set Notebook1c = 'Notebook1 -u 3 -I Notebook1c.Dat -S Notebook1c.Scr -O Notebook1c.prt -V Notebook1c.vis'

set Notebook1d = 'Notebook1 -u 4 -I Notebook1d.Dat -S Notebook1d.Scr -O Notebook1d.prt -V Notebook1d.vis'

set Notebook1e = 'Notebook1 -u 5 -I Notebook1e.Dat -S Notebook1e.Scr -O Notebook1e.prt -V Notebook1e.vis'

set Notebook1f = 'Notebook1 -u 6 -I Notebook1f.Dat -S Notebook1f.Scr -O Notebook1f.prt -V Notebook1f.vis -xrm "*Notebook1.layoutDirection: RIGHT_TO_LEFT" '

set Notebook3a = 'Notebook3 -u XmFrame -I Notebook3a.Dat -S Notebook3a.Scr -O Notebook3a.prt -V Notebook3a.vis'

set Notebook3b = 'Notebook3 -u XmForm -I Notebook3b.Dat -S Notebook3b.Scr -O Notebook3b.prt -V Notebook3b.vis'

set Notebook3c = 'Notebook3 -u XmFrame -I Notebook3c.Dat -S Notebook3c.Scr -O Notebook3c.prt -V Notebook3c.vis'

set Notebook3d = 'Notebook3 -u XmBulletinBoard -I Notebook3d.Dat -S Notebook3d.Scr -O Notebook3d.prt -V Notebook3d.vis'

set Notebook6 = 'Notebook6 -u XmFrame'


foreach test ("$Notebook1a" "$Notebook1b" "$Notebook1c" "$Notebook1d" "$Notebook1e" "$Notebook1f" Notebook2 "$Notebook3a" "$Notebook3b" "$Notebook3c" "$Notebook3d" Notebook4 Notebook5 "$Notebook6")
