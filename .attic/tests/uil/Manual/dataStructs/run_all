# HISTORY

#
#	This shell script will run each of the UIL Data Structures tests
#	and redirect the output to the file 'dataStruct.out'
#

echo ""
echo "Running UIL Data Structure tests."
echo "Output will be redirected to the file dataStruct.out."

echo "These are the results of the UIL Data Structures tests:" > dataStruct.out
echo "" >> dataStruct.out
echo "" >> dataStruct.out

echo "	."
echo "---------------------------- Simple Data Structures ----------------------------" >> dataStruct.out

Simple >> dataStruct.out

echo "	."
echo "------------------------ Compound String Data Structure ------------------------" >> dataStruct.out

CompoundS >> dataStruct.out

echo "	."
echo "----------------------------- Font Data Structure ------------------------------" >> dataStruct.out

Font >> dataStruct.out

echo "	."
echo "---------------------------- Pixmap Data Structure -----------------------------" >> dataStruct.out

Pixmap >> dataStruct.out

echo "	."
echo "---------------------------- Table Data Structures -----------------------------" >> dataStruct.out

Tables >> dataStruct.out

echo "	."
echo "------------------------------- Values Test -----------------------------" >> dataStruct.out

Values >> dataStruct.out

echo "UIL Data Structure tests have completed."
echo ""

#
#  End Run script
#
