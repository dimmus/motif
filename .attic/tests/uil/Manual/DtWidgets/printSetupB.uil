module Test
        version = 'v1.0'
        names = case_sensitive

  procedure
	  Report_Dtcallback();

  value
	  wheat			     : color('wheat');
	  navy			     : color('navy');

  object                                  ! The main window widget.
          S_MAIN_WIDGET : XmBulletinBoard {

              arguments {
                  XmNx = 0;
                  XmNy = 0;
              };
    	      controls {
                  DtPrintSetupBox    		Print_Setup_Box;
   	      };
  	  };

  value
        turquoise: color ('turquoise');
        black: color ('black');
        violet: color ('violet');

  object
        Print_Setup_Box :  DtPrintSetupBox {
            arguments {
                XmNbackground =  turquoise;
		XmNheight = 200;
		XmNwidth = 300;
		DtNdescription = "Default Xp Printer";
		DtNcopies = 3;
		DtNfileName = "/tmp/printSetup_test";
		DtNminimizeButtons = false;
		DtNprintDestination = DtPRINT_TO_FILE;
		DtNprinterName = "fed";
		DtNprintSetupMode = DtPRINT_SETUP_XP;
		DtNworkAreaLocation = DtWORK_AREA_TOP_AND_BOTTOM;
		};
	      callbacks {
		DtNcancelCallback = procedure Report_Dtcallback();

            };
        };


end module;
